import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		double v1 = 0;
		double v2 = 0;
		double v3 = 0;
		double media = 0;
		for(int i = 0; i < N; i++) {
			v1 = sc.nextDouble();
			v2 = sc.nextDouble();
			v3 = sc.nextDouble();
			media = v1 + v2 + v3 / 10;
			System.out.println(media);
		}
		
		
		
		sc.close();

	}

}
